{
    "name": "danpalmieri/laravel-subscription-engine",
    "description": "Laravel Subscription Engine is a flexible plans and subscription management system for Laravel.",
    "type": "library",
    "keywords": [
        "plan",
        "value",
        "laravel",
        "feature",
        "database",
        "recurring",
        "subscription"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Palmieri"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^v11.0",
        "doctrine/dbal": "^4.1.1"
    },
    "require-dev": {
        "illuminate/console": "^11.0",
        "illuminate/container": "^11.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "DanPalmieri\\SubscriptionEngine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanPalmieri\\SubscriptionEngine\\Tests\\": "tests",
            "DanPalmieri\\SubscriptionEngine\\Tests\\Database\\Factories\\": "tests/Database/Factories"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanPalmieri\\SubscriptionEngine\\SubscriptionEngineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
